#GRABBING THE SHORTEST CITY, GRABS FIRST AND ONLY ONE
SELECT CITY, LENGTH(CITY)
FROM STATION
ORDER BY LENGTH(CITY),CITY LIMIT 1;

#GRABBING THE LONGEST CITY, GRABS FIRST AND ONLY ONE
SELECT CITY, LENGTH(CITY)
FROM STATION
ORDER BY LENGTH(CITY)DESC, CITY LIMIT 1;
